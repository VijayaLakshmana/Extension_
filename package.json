{
  "name": "inline-suggestion",
  "displayName": "Inline-suggestion",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.90.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
  ],
  "main": "./out/extension.js",
  "contributes": {
  "viewsContainers": {
      "activitybar": [
          {
              "id": "mySidebar",
              "title": "My Sidebar",
              "icon": "./media/per.jpg"
          }
      ]
  },
  "views": {
      "mySidebar": [
          {
              "type":"webview",
              "id": "myInput.myView",
              "name": "Input View"
          }
      ]
  },
  "submenus": [
    {
      "id": "projectX.submenu",
      "label": "Project X"
    }
  ],
  "commands": [
    {
      "command": "vscode-sparkup.biasFreeLanguage",
      "title": "Explain"
    },
    {
      "command": "vscode-sparkup.bias",
      "title": "Refactor"
    },
    {
      "command": "vscode-sparkup.biasFree",
      "title": "Fix"
    },
    {
      "command": "vscode-sparkup.biasFreeOptimize",
      "title": "Optimize"
    },
    {
      "command": "extension.populateInputField",
      "title": "Send to prompt"
    }
  ],
  "menus": {
    "editor/context": [
      {
        "submenu": "projectX.submenu",
        "group": "Project X",
        "when": "editorFocus"
      }
    ],
    "projectX.submenu": [
      {
        "command": "vscode-sparkup.biasFreeLanguage"
      },
      {
        "command": "vscode-sparkup.bias"
      },
      {
        "command": "vscode-sparkup.biasFree"
      },
      {
        "command": "vscode-sparkup.biasFreeOptimize"
      },
      {
        "command": "extension.populateInputField"
      }
    ]
  }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "format": "prettier --write src/**/*.ts src/*.ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@types/vscode": "^1.90.0",
    "@typescript-eslint/eslint-plugin": "^7.11.0",
    "@typescript-eslint/parser": "^7.11.0",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0",
    "eslint": "^8.57.0",
    "prettier": "^3.3.2",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "axios": "^1.7.2",
    "dotenv": "^16.4.5"
  }
}
